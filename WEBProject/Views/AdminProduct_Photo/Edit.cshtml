@model Product_Photo
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <h1>Edit</h1>
    <form asp-action="Edit" method="post">
        <input type="hidden" name="PhotoID" value="@Model.PhotoID" />
        <input type="hidden" name="ArtNR" value="@Model.ArticleNumber" />
        New PhotoID:<input type="number" name="newPhotoID" />
        New ArticleNumber:<input type="number" name="newArtNR" />
        <input type="submit" value="Submit" />
    </form>
</div>
