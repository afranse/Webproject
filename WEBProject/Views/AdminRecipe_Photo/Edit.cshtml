@model Recipe_Photo
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <h1>Edit</h1>
    <form asp-action="Edit" method="post">
        <input type="hidden" name="PhotoID" value="@Model.PhotoID" />
        <input type="hidden" name="RecipeID" value="@Model.RecipeID" />
        New PhotoID:<input type="number" name="newPhotoID" />
        New RecipeID:<input type="number" name="newRecipeID" />
        <input type="submit" value="Submit" />
    </form>
</div>
