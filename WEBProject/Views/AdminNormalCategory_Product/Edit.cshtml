@model NormalCategory_Product
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <h1>Edit</h1>
    <form asp-action="Edit" method="post">
        <input type="hidden" name="CategoryID" value="@Model.CategoryID" />
        <input type="hidden" name="ArtNR" value="@Model.ArticleNumber" />
        New CategoryID:<input type="number" name="newCategoryID" />
        New ArticleNumber:<input type="number" name="newArtNR" />
        <input type="submit" value="Submit" />
    </form>
</div>
