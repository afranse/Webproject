@model TypeCategory_Recipe
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <h1>Edit</h1>
    <form asp-action="Edit" method="post">
        <input type="hidden" name="TypeID" value="@Model.TypeID" />
        <input type="hidden" name="RecipeID" value="@Model.RecipeID" />
        New TypeID:<input type="number" name="newTypeID" />
        New RecipeID:<input type="number" name="newRecipeID" />
        <input type="submit" value="Submit" />
    </form>
</div>
