@model Text_News
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <h1>Edit</h1>
    <form asp-action="Edit" method="post">
        <input type="hidden" name="TextID" value="@Model.TextID" />
        <input type="hidden" name="NewsID" value="@Model.NewsID" />
        New TextID:<input type="number" name="newTextID" />
        New NewsID:<input type="number" name="newNewsID" />
        <input type="submit" value="Submit" />
    </form>
</div>
